<body>
	<div class="header"></div>
	<div class="container">
		<div class="row common-row logRowHeight">
			<div class="person-c-headImage">
					<!-- <img src="../img/qq.png" alt="" >
					<span>耶和华是我的的牧者！</span> -->
			</div>
			<div class="col-md-12">
				<div class="tabbable" id="tabs-1">
					<ul class="nav nav-tabs">
						<li class="active" id="allQuesition">
							<a data-toggle="tab" href="#panel-1" >全部问题</a>
						</li>
						<li  id="noSoluted">
							<a data-toggle="tab" href="#panel-2" >待解决</a>
						</li>
						<li id = "soluted">
							<a data-toggle="tab" href="#panel-3" >已解决</a>
						</li>
					
					</ul>
					<div class="tab-content myWit-tab-content">
						<div class="tab-pane active " id="panel-1">
							<div class="WitnessContent"><!-- 我的日志表格开始 -->
								<table id="fSAllQtDg" class="display myLogTable" cellspacing="0" width="100%"></table>
							</div><!-- 我的日志表格结束 -->
						</div>
						<div class="tab-pane " id="panel-2">
							<div class="WitnessContent"><!-- 我的日志表格开始 -->
								<table id="myLogDatagridQT" class="display myLogTable" cellspacing="0" width="100%"></table>
							</div><!-- 我的日志表格结束 -->
						</div>
						<div class="tab-pane l-tab-pane-index" id="panel-3">
						</div>
					</div>
				</div>
				<!-- 模态框开始 -->
				<!-- data-target触发模态弹出窗元素 -->
				
				<!-- 模态弹出窗内容 -->
				<div class="modal fade" id="mymodal-data" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
					<div class="modal-dialog" id="">
						<div class="modal-content" style="width: 800px;">
							<div class="modal-header">
								<button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
								<h4 class="modal-title">我的日志</h4>
							</div>
							<div class="modal-body">
								<form role="form">
								  <div class="form-group">
								   	<label for="logTitle">标题</label>
								    <input type="text" value="" name="logTitle" class="form-control logTitle" placeholder="请输入见证/日志标题">
									<label for="logContent">内容</label>
									<!-- <textarea class="form-control logContent" rows="3" name="logContent" ></textarea> -->
								  </div>
								</form>  
								
							</div>
							<script id="logEditor" type="text/plain" style="width:767px; height: 170px; margin:0px auto 40px 15px;"></script>
							<div class="modal-footer">
								<button type="button" class="btn btn-default" data-dismiss="modal">取消</button>
								<button type="button" class="btn btn-primary logBtn" id="addLog">发表见证</button>
								<button type="button" class="btn btn-primary logBtn" id="addLogQT" style="display:none;">发表灵修日志</button>
							</div>
						</div>
					</div>
